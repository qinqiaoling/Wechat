/**index.wxss**/
@import "../weui/weui.wxss";
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.editwidth{
  width: 100%;
  height: 100%;
}
.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}
.userinfo-nickname {
  color: #aaaaaa;
}
.usermotto {
  margin-top: 100px;
}
.header{
  padding: 80rpx 0;
}
.header image{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
}
.content{
  border-top: 1px solid #cccccc;
  padding: 20rpx 0;
}
.content text{
  display: block;
  padding: 10rpx 0;
}
.accredit{
  border-radius: 80rpx;
  width: 100%;
  font-size: 36rpx;
}
.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}
.userinfo-nickname {
  color: #aaaaaa;
}
.usermotto {
  margin-top: 100px;
}
.indexpage{
  width: 100%;
  height: 100%;
}
button{
  margin-bottom: 30rpx;
}
button:last-child{
  margin-bottom: 0;
}
.page-section-title{
  padding: 0;
}
.head-swiper swiper{
  height: 480rpx;
}
.swiper-item{
  display: block;
  height: 100%;
  text-align: center;
  overflow: hidden;
}
.swiper-item image{
  height: 100%;
  width: 100%;
  overflow: hidden;
}
.page-section-title{
  margin-top: 60rpx;
  position: relative;
}
.info{
  position: absolute;
  right: 0;
  color: #353535;
  font-size: 30rpx;
}
.page-foot{
  margin-top: 50rpx;
}
.navbar{
  padding: 30rpx 0;
}
.navbar .page-section-spacing {
  box-sizing:border-box;
}
.navbar .flex-wrp {
  display:flex;
  flex-direction:row;
  flex-wrap: wrap;
}
.navbar .flex-item {
  flex: 0 0 25%;
  font-size:28rpx;
  position:relative;
  align-items:center;
  justify-content:center;
  color:#000000;
}
.navbar .navurl {
  display: block;
  text-align: center;
  padding-bottom: 30rpx;
}
.navbar .navurl image{
  width: 100rpx;
  height: 100rpx;
}
.navbar .navurl text{
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.modules{
  /* padding-bottom: 20rpx; */
}
.modules_title{
  font-size: 36rpx;
  text-align: center;
  background: #005eb4;
  color: #ffffff;
  height: 70rpx;
  line-height: 70rpx;
}
.modules_title text{
  padding: 0 30rpx;
}
.modules_flex{
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
}
.modules_flex_view{
  flex: 1;
}
.modules_flex_view.product{
  flex: 0 0 50%;
  font-size:28rpx;
  position:relative;
  align-items:center;
  justify-content:center;
  color:#000000;
  overflow: hidden;
}
.modules_flex_view.product .product_nav{
  display: block;
  text-align: center;
  padding: 20rpx;
  height: 400rpx;
  overflow: hidden;
}
.modules_flex_view.product .product_nav text{
  padding: 20rpx 20rpx 0 20rpx;
  display: block;
  text-align: center;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.nav_wrap{
  display: flex;
  width: 100%;
  overflow: hidden;
}
.nav_wrap_flex{
  flex: 1;
  align-items: center;
  justify-content: center;
  border: 1px solid #cccccc;
  box-sizing: border-box;
  overflow: hidden;
  height: 360rpx;
}
.modules_flex_view.product .product_nav image{
  max-width: 100%;
  max-height: 100%;
  width: 100%;
  height: 360rpx;
}
.modules_more{
  text-align: center;
  padding: 30rpx 0;
}
.modules_more .modules_more_a{
  border:1px solid #005eb4;
  border-radius: 30rpx;
  font-size: 30rpx;
  color: #000000;
  width: 50%;
  background: #ffffff;
  height: 60rpx;
  line-height: 60rpx;
  display: inline-block;
}
.modules_flex_view.news_trends{
  flex: 0 0 100%;
  overflow: hidden;
}
.modules_flex_view.news_trends .news_trends_nav{
  border-bottom: 1px solid #cccccc;
  padding: 20rpx 40rpx;
  font-size: 30rpx;
}
.news_trends_nav_title{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.news_trends_nav_p{
  color: #767676;
  overflow: hidden;
  text-overflow: ellipsis;
  /* white-space: nowrap; */
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.modules_flex_view.ProjectCases{
  flex: 0 0 100%;
  overflow: hidden;
}
.Project_Cases{
  padding: 20rpx;
  font-size:30rpx;
  border-bottom: 1px solid #cccccc;
  display: block;
  overflow: hidden;
}
.Project_Cases_flex{
  display: flex;
  flex-direction: row;
}
.Project_Cases_l{
  width: 200rpx;
  height: 200rpx;
  overflow: hidden;
  margin-right: 20rpx;
}
.Project_Cases_l image{
  width: 200rpx;
  height: 200rpx;
}
.Project_Cases_r{
  flex: 1;
  overflow: hidden;
}
.Project_Cases_title{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.Project_Cases_font{
  color: #767676;
  overflow: hidden;
  text-overflow: ellipsis;
  /* white-space: nowrap; */
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
} 
.About{
  padding: 20rpx;
}
.About .Project_Cases_l{
  width: 300rpx;
  overflow: hidden;
}
.About .Project_Cases_l image{
  width: 300rpx;
  max-height:300rpx;
  display: inline-block;
  overflow: hidden;
}
.About .Project_Cases_r .Project_Cases_title{
  color: #000000;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: normal;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 4;
  font-size: 30rpx;
}
.contact_us{
  padding: 20rpx;
  font-size: 30rpx;
}
.contact_block{
  display: block;
}
.contact_block text:first-child{
  display: inline-block;
}
.contact_block text:last-child{
  display: inline-flex;
}
.contact_us_rot_line{
  padding: 20rpx 20rpx 40rpx 20rpx;
}
.contact_us_rot_line button{
  background:#ff8500;
  color: #ffffff;
  border-radius: 16rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  border: 1px solid #ff8500;
  padding: 0;
  font-size: 36rpx;
}